{
	"type": "module",
	"name": "temporal-polyfill-lite",
	"version": "0.1.0",
	"description": "Lightweight Temporal polyfill supporting all time zones and basic calendars",
	"keywords": [
		"Temporal"
	],
	"author": "fabon <syobon.hinata.public@gmail.com> (https://www.fabon.info/)",
	"license": "MIT",
	"exports": {
		".": "./dist/index.js",
		"./global": "./dist/global.js",
		"./shim": "./dist/shim.js"
	},
	"files": [
		"CHANGELOG.md",
		"LICENSE",
		"README.md",
		"dist"
	],
	"sideEffects": ["./dist/global.js"],
	"homepage": "https://github.com/fabon-f/temporal-polyfill-lite#readme",
  "bugs": {
    "url": "https://github.com/fabon-f/temporal-polyfill-lite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabon-f/temporal-polyfill-lite.git"
  },
	"devDependencies": {
		"@js-temporal/temporal-test262-runner": "github:js-temporal/temporal-test262-runner#main",
		"@rollup/plugin-strip": "^3.0.4",
		"@rollup/plugin-terser": "^0.4.4",
		"@tsconfig/strictest": "^2.0.8",
		"@types/node": "^25.0.10",
		"@typescript/native-preview": "7.0.0-dev.20260124.1",
		"oxfmt": "0.26.0",
		"oxlint": "^1.41.0",
		"rolldown": "1.0.0-rc.1",
		"tzdata": "^1.0.47",
		"unplugin-isolated-decl": "^0.15.7",
		"vitest": "^4.0.18"
	},
	"scripts": {
		"build": "node script/build.ts",
		"test": "vitest",
		"test262": "node script/test262.ts",
		"typecheck": "tsgo",
		"lint": "oxfmt --check . && oxlint . --report-unused-disable-directives"
	},
	"pnpm": {
		"overrides": {
			"vite": "beta"
		}
	},
	"packageManager": "pnpm@10.28.1"
}
