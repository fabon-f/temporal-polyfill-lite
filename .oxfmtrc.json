{
	"$schema": "./node_modules/oxfmt/configuration_schema.json",
	"useTabs": true,
	"tabWidth": 2,
	"ignorePatterns": [
		"test262",
		"pnpm-lock.yaml",
		"*.json"
	],
	"experimentalSortImports": {
		"groups": [
			["side_effect"],
			["builtin"],
			["external"],
			["parent"],
			["sibling", "index"],
		],
		"newlinesBetween": false
	}
}
